{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ngrc-platform": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/ngrc-platform",
      "sourceRoot": "apps/ngrc-platform/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ngrc-platform",
            "index": "apps/ngrc-platform/src/index.html",
            "main": "apps/ngrc-platform/src/main.ts",
            "polyfills": "apps/ngrc-platform/src/polyfills.ts",
            "tsConfig": "apps/ngrc-platform/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/ngrc-platform/src/favicon.ico",
              "apps/ngrc-platform/src/assets"
            ],
            "styles": [
              "apps/ngrc-platform/src/custom-theme.scss",
              "apps/ngrc-platform/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ngrc-platform/src/environments/environment.ts",
                  "with": "apps/ngrc-platform/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngrc-platform:build",
            "proxyConfig": "apps/ngrc-platform/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngrc-platform:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrc-platform:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ngrc-platform/jest.config.js",
            "tsConfig": "apps/ngrc-platform/tsconfig.spec.json",
            "setupFile": "apps/ngrc-platform/src/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ngrc-platform/tsconfig.app.json",
              "apps/ngrc-platform/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ngrc-platform-e2e": {
      "projectType": "application",
      "sourceRoot": "apps/ngrc-platform-e2e/src",
      "root": "apps/ngrc-platform-e2e",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrc-platform-e2e/cypress.json",
            "tsConfig": "apps/ngrc-platform-e2e/tsconfig.e2e.json",
            "devServerTarget": "ngrc-platform:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrc-platform:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/ngrc-platform-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "docs": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/docs",
      "sourceRoot": "apps/docs/src",
      "prefix": "ngrc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/docs",
            "index": "apps/docs/src/index.html",
            "main": "apps/docs/src/main.ts",
            "polyfills": "apps/docs/src/polyfills.ts",
            "tsConfig": "apps/docs/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/docs/src/favicon.ico", "apps/docs/src/assets"],
            "styles": [
              "apps/ngrc-platform/src/custom-theme.scss",
              "apps/docs/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/docs/src/environments/environment.ts",
                  "with": "apps/docs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "docs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "docs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "docs:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/docs/tsconfig.app.json",
              "apps/docs/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/docs/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/docs/jest.config.js",
            "tsConfig": "apps/docs/tsconfig.spec.json",
            "setupFile": "apps/docs/src/test-setup.ts"
          }
        }
      }
    },
    "docs-e2e": {
      "root": "apps/docs-e2e",
      "sourceRoot": "apps/docs-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/docs-e2e/cypress.json",
            "tsConfig": "apps/docs-e2e/tsconfig.e2e.json",
            "devServerTarget": "docs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "docs:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/docs-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/docs-e2e/**"]
          }
        }
      }
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "prefix": "server",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": ["apps/server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/server/tsconfig.app.json",
              "apps/server/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/server/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/server/jest.config.js",
            "tsConfig": "apps/server/tsconfig.spec.json"
          }
        }
      }
    },
    "nrf24": {
      "root": "libs/nrf24",
      "sourceRoot": "libs/nrf24/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/nrf24/tsconfig.lib.json",
              "libs/nrf24/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/nrf24/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/nrf24/jest.config.js",
            "tsConfig": "libs/nrf24/tsconfig.spec.json"
          }
        }
      }
    },
    "dualshock": {
      "root": "libs/dualshock",
      "sourceRoot": "libs/dualshock/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/dualshock/tsconfig.lib.json",
              "libs/dualshock/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/dualshock/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/dualshock/jest.config.js",
            "tsConfig": "libs/dualshock/tsconfig.spec.json"
          }
        }
      }
    },
    "dualshock-shared": {
      "root": "libs/dualshock-shared",
      "sourceRoot": "libs/dualshock-shared/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/dualshock-shared/tsconfig.lib.json",
              "libs/dualshock-shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/dualshock-shared/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/dualshock-shared/jest.config.js",
            "tsConfig": "libs/dualshock-shared/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "defaultProject": "ngrc-platform",
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  }
}
